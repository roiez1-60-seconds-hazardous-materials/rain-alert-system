{
  "version": 2,
  "name": "rain-alert-dashboard",
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "public": true,
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
